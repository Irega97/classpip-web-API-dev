{
  "name": "Cuestionario",
  "plural": "Cuestionarios",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "string",
      "required": true
    },
    "publico": {
      "type": "boolean",
      "required": false,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "preguntas": {
      "type": "hasMany",
      "model": "Pregunta",
      "foreignKey": "cuestionarioId",
      "options": {
        "nestRemoting": true
      },
      "through": "PreguntaDelCuestionario"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
