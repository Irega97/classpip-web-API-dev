{
  "name": "Coleccion",
  "plural": "Colecciones",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "imagenColeccion": {
      "type": "string",
      "required": true
    },
    "dosCaras": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "recomendacion": {
      "type": "string"      
    }
  },
  "validations": [],
  "relations": {
    "cromos": {
      "type": "hasMany",
      "model": "Cromo",
      "foreignKey": "coleccionId"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
